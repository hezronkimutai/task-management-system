<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Suppress H2 database console vulnerabilities in development/test environment -->
    <suppress>
        <notes><![CDATA[
        H2 database console is only used in development/test environments.
        Production uses different database configurations.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.h2database/h2@.*$</packageUrl>
        <cve>CVE-2018-10054</cve>
        <cve>CVE-2018-14335</cve>
        <cve>CVE-2021-42392</cve>
        <cve>CVE-2022-23221</cve>
    </suppress>
    
    <!-- Suppress Spring Boot test-related vulnerabilities -->
    <suppress>
        <notes><![CDATA[
        Test dependencies are not deployed to production.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/spring-boot-starter-test@.*$</packageUrl>
        <cvssBelow>7.0</cvssBelow>
    </suppress>
    
    <!-- Add more suppressions as needed for specific vulnerabilities -->
</suppressions>
